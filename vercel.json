{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd whatsapp-service/frontend && npm install && npm run build",
  "outputDirectory": "whatsapp-service/frontend/.next",
  "installCommand": "cd whatsapp-service/frontend && npm install",
  "framework": "nextjs",
  "functions": {
    "whatsapp-service/frontend/api/**/*.ts": {
      "runtime": "nodejs22.x"
    },
    "whatsapp-service/frontend/api/**/*.js": {
      "runtime": "nodejs22.x"
    }
  }
}
